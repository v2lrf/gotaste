<nav class="navigation" role="navigation">
  <div>
    <%= link_to 'https://gotaste.dk' do %>
      <%= image_tag 'https://gotaste.dk/static/gotaste-logo.svg', width: "100px" %>
    <% end %>
  </div>

  <% resources =  AdminPages.routes(Administrate::Namespace.new(namespace)) %>
  <% resources.each do |resource| %>
    <%= link_to(
      display_resource_name(resource),
      [namespace, resource_index_route_key(resource)],
      class: "navigation__link navigation__link--#{nav_link_state(resource)}"
    ) %>
  <% end %>
  <hr>
  <%= link_to "Sign out", destroy_user_session_path, method: :delete, class: "navigation__link navigation__link" %>
</nav>

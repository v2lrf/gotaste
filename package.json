{
  "name": "govinu",
  "private": true,
  "engines": {
    "node": "10.14.1"
  },
  "scripts": {
    "dev": "next ./client",
    "build": "next build ./client",
    "start": "next start ./client -p $PORT",
    "heroku-postbuild": "npm run build",
    "lint": "eslint -c .eslintrc.json --ext .jsx,.js,.js.erb app/javascript",
    "prestorybook": "tailwind build ./app/javascript/css/application.css -c ./app/javascript/css/tailwind.js -o ./.storybook/application.css",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "NODE_ENV=test jest"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@storybook/react": "^3.4.11",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link": "^1.2.6",
    "apollo-link-context": "^1.0.12",
    "apollo-link-error": "^1.1.5",
    "apollo-link-http": "^1.5.9",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "cloudinary-react": "^1.1.0",
    "dotenv": "^6.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "moment": "^2.23.0",
    "next": "^8.0.0",
    "next-with-apollo": "^3.3.2",
    "places.js": "^1.15.1",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.8.1",
    "rollbar": "^2.5.2",
    "tailwindcss": "^0.7.3",
    "webpack": "3.12.0"
  },
  "devDependencies": {
    "babel-jest": "^24.1.0",
    "babel-preset-env": "^1.7.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "webpack-dev-server": "2.11.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/client/jest.setup.js"
    ],
    "roots": [
      "<rootDir>/client"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/client/.next/",
      "<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "test": {
        "presets": [
          "env",
          "react"
        ]
      }
    }
  }
}

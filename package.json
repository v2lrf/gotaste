{
  "name": "govinu",
  "private": true,
  "engines": {
    "node": "10.14.1"
  },
  "scripts": {
    "predev": "node client/lib/getFragmentTypes.js",
    "dev": "node ./client/server.js",
    "prebuild": "NODE_ENV=production node client/lib/getFragmentTypes.js && node client/build-sitemap.js",
    "build": " next build ./client",
    "start": "NODE_ENV=production node ./client/server.js",
    "heroku-postbuild": "npm run build",
    "lint": "eslint -c .eslintrc.json --ext .jsx,.js client",
    "prestorybook": "tailwind build ./client/styles/styles.css -c ./client/tailwind.config.js -o ./.storybook/application.css && node ./.storybook/prepare-storybook.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "NODE_ENV=test jest",
    "graphql:dump": "graphql-inspector introspect http://localhost:3000/graphql --write schema.graphql"
  },
  "graphql-inspector": {
    "diff": true,
    "schema": {
      "ref": "master",
      "path": "schema.graphql"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/pro-light-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@graphql-inspector/cli": "^1.14.0",
    "@storybook/react": "^3.4.12",
    "@zeit/next-css": "1.0.2-canary.2",
    "apollo": "^2.6.2",
    "apollo-boost": "^0.3.1",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.17",
    "apollo-link-http": "^1.5.14",
    "apollo-link-persisted-queries": "^0.2.2",
    "apollo-server-express": "^2.4.8",
    "autoprefixer": "^9.5.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "cloudinary-core": "^2.6.2",
    "cloudinary-react": "^1.1.0",
    "dotenv": "^7.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "express": "^4.16.4",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "next": "^8.0.4",
    "nprogress": "^0.2.0",
    "places.js": "^1.16.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.3",
    "react-apollo-hooks": "^0.4.4",
    "react-dom": "^16.8.6",
    "react-map-gl": "^4.1.1",
    "react-markdown": "^4.0.6",
    "rollbar": "^2.5.4",
    "sitemap": "^2.1.0",
    "tailwindcss": "^0.7.4"
  },
  "devDependencies": {
    "babel-jest": "^24.1.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-preset-env": "^1.7.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^23.6.0",
    "prettier": "^1.16.4",
    "webpack": "4.29.6"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/client/jest.setup.js"
    ],
    "roots": [
      "<rootDir>/client"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/client/.next/",
      "<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "test": {
        "presets": [
          "env",
          "react"
        ],
        "plugins": [
          "dynamic-import-node"
        ]
      }
    }
  }
}

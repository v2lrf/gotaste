{
  "name": "govinu",
  "private": true,
  "engines": {
    "node": "10.14.1"
  },
  "scripts": {
    "dev": "node ./client/server.js",
    "build": "NODE_ENV=production node client/build-rss-feed.js && next build ./client",
    "start": "NODE_ENV=production node ./client/server.js",
    "heroku-postbuild": "npm run build",
    "lint": "eslint -c .eslintrc.json --ext .jsx,.js client",
    "prestorybook": "tailwind build ./client/styles/styles.css -c ./client/tailwind.config.js -o ./.storybook/application.css && node ./.storybook/prepare-storybook.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "NODE_ENV=test jest"
  },
  "graphql-inspector": {
    "diff": true,
    "schema": {
      "ref": "head/master",
      "path": "app/graphql/schema.graphql"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@graphql-inspector/cli": "^0.13.3",
    "@storybook/react": "^3.4.11",
    "@zeit/next-css": "1.0.2-canary.2",
    "apollo-boost": "^0.3.1",
    "apollo-link": "^1.2.9",
    "apollo-link-context": "^1.0.14",
    "apollo-link-http": "^1.5.12",
    "autoprefixer": "^9.4.10",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "cloudinary-core": "^2.6.2",
    "cloudinary-react": "^1.1.0",
    "dotenv": "^6.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.0",
    "moment": "^2.24.0",
    "next": "^8.0.3",
    "next-with-apollo": "^3.3.2",
    "nprogress": "^0.2.0",
    "places.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-apollo": "^2.5.1",
    "react-content-loader": "^4.0.1",
    "react-dom": "^16.8.3",
    "react-map-gl": "^4.0.14",
    "rollbar": "^2.5.3",
    "rss": "^1.2.2",
    "tailwindcss": "^0.7.4"
  },
  "devDependencies": {
    "apollo": "^2.5.3",
    "babel-jest": "^24.1.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-preset-env": "^1.7.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^23.6.0",
    "prettier": "^1.16.4",
    "webpack": "4.16.3"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/client/jest.setup.js"
    ],
    "roots": [
      "<rootDir>/client"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/client/.next/",
      "<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "test": {
        "presets": [
          "env",
          "react"
        ],
        "plugins": [
          "dynamic-import-node"
        ]
      }
    }
  }
}
